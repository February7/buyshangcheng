/* components/tabs/tabs.wxss */
.tabs-title{
  width: 100%;
  height: 80rpx;
  display: flex;
}
.tabs-title-item{
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
}
.active{
  color: #B03A2E;
  border-bottom: 2px solid currentColor;
  box-sizing: border-box;
}